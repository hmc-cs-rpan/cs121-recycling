<h2> New General Information Page </h2>
<hr>

<%= form_for @article, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_area :title %>
  <%= f.label :content %>
  <%= f.text_area :content %>
	<%= f.label :image %>
	<%= f.file_field :image %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <%= f.submit %>

<% end %>